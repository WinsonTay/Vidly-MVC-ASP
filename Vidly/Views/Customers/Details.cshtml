@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var SubcribeToNewsLetter = Model.IsSubcribedToNewsLetter ? "Yes" : "No";


}
 

<h2>@Model.name</h2>
<h3>Membership Type: @Model.MembershipType.Name</h3>
@if (@Model.BirthDate.HasValue)
{
<h3>Birthdate: @Model.BirthDate.Value.ToString("dd/MM/yyyy") </h3>

}

